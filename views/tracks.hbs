<ul>
{{#each tracks}}
<li class="artistsLi" >
      <h1>{{this.name}}</h1>
    {{#if this.preview_url}}
         <p>No tracks available</p>
      {{else}}
      <td>
<audio controls>

<source src="{{this.preview_url}}" type="audio/mpeg">
      </audio>
      </td>
 
    {{/if}}
</li>
{{/each}}
</ul>




 